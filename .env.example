#!/bin/bash
# Environment variables for Document AI extraction system

# Mistral API Key for document processing
export MISTRAL_API_KEY=your_mistral_api_key_here

# Mistral API URL (optional)
export MISTRAL_API_URL=https://api.mistral.ai/v1/chat/completions

# Mistral model to use (optional)
export MISTRAL_MODEL=mistral-large-latest

# AWS Region for deployment (used by deploy.sh)
export AWS_REGION=us-east-1
